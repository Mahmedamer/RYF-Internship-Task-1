@model IEnumerable<NYFInter.Entry>

@{
    ViewBag.Title = "MyEntries";
    Layout = "~/Views/User/_Layout.cshtml";
}

<h2>My Entries</h2>

<p>
    @Html.ActionLink("Create New", "CreateEntry")
</p>
<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditEntry", new { id=item.Id }) |
            @Html.ActionLink("Details", "EntryDetails", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteEntry", new { id=item.Id })
        </td>
    </tr>
}

</table>
